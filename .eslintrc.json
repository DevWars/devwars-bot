{
    "extends": "airbnb-base",
    "env": {
        "browser": false,
        "jest": true
    },
    "rules": {
        "indent": ["error", 4, {"SwitchCase": 1}],
        "omitLastInOneLineBlock": true,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-restricted-syntax": 0,
        "arrow-body-style": 0,
        "prefer-destructuring": 0,
        "object-curly-newline": 0,
        "consistent-return": 0,
        "no-param-reassign": 0
    }
}
